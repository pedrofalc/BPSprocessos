<div  STYLE="position:absolute; TOP:150px; LEFT:250px; WIDTH:1000px; HEIGHT:10px"  >




 <div class="col-md-9">

<h2 align="center"><FONT color="Green"><%= notice %> </FONT> <h2>

<div class="panel-body"  STYLE="position:absolute; TOP:-100px; LEFT:-100px; WIDTH:1000px; HEIGHT:10px">
               <div class="panel panel-primary" align="center">
                    <div class="panel-heading">
                        <h2 align="center"><i class="fa fa-fw fa-check"></i> <%= @selection.name %></h2>
                    </div>

        <div class="row" align="center">



                       <h4>Final: <%= @selection.end %> <i class="fa fa-calendar fa-2"></i><h4>
                <h4> Id do Professor: <%= @selection.owner %> <i class="fa fa-tag fa-2"></i>
                       <h4>
                            
          <div>                    
<table  >
  <thead>
    <tr>
      <th WIDTH=100 height=50  >Index</th>
      <th WIDTH=200 height=50 >Estudantes</th>
      <th WIDTH=100>Email</th>
      <th WIDTH=150>Histórico</th>
      <th WIDTH=150>Currículo</th>
    </tr>
  </thead>

  <tbody>
    <% @students.each_with_index do |student,index| %>
      <tr>
        <td WIDTH=100 height=50 ><%=index+1%></td>
        <td  WIDTH=200 height=50  ><%= student.name %><br/></td>
        <td WIDTH=100><%= student.email %></td>
        <td WIDTH=150>  <a class="btn btn-primary btn-lg" href="<%= student.history_url%>">Histórico</a></td>
        <td WIDTH=150><a class="btn btn-primary btn-lg"  href="<%= student.resume_url %>">Currículo</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>  







                             </div>

                         <%= link_to 'Edit', edit_selection_path(@selection),class: "btn btn-default" %>
<%= link_to 'Back', selections_path, class: "btn btn-default" %>

                   </div>

</div>
</div>
</div>
</div>
